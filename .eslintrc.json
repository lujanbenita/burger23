{
	"env": {
		"browser": true,
		"es2020": true,
		"commonjs": true,
		"node": true
	},
	"extends": ["prettier", "plugin:react/recommended"],
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module"
	},
	"rules": {
		"indent": ["warn", "tab"],
		"react/react-in-jsx-scope": "off",
		"react/prop-types": "off"
	},
	"parser": "@babel/eslint-parser"
}
